<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eric.timicaller31">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DownloadActivity" />
        <activity android:name=".Setting.SettingActivity" />
        <activity android:name=".MyEvent.AddNewAlarm" />

        <receiver android:name=".MyEvent.AlarmReceiver" />

        <activity android:name=".MyEvent.RingActivity" />
        <activity android:name=".MyEvent.Edit_MyAlarmActivity" />
        <activity android:name=".MyEvent.Detail_MyAlarmActivity" />
        <activity android:name=".MyTimer.AddNewTimerActivity" />
        <activity android:name=".UploadRoom.AddNewRoomActivity" />
        <activity android:name=".UploadRoom.URoomActivity" />
        <activity android:name=".UploadRoom.AddNewUEvent" />
        <activity android:name=".DownloadRoom.DRoomActivity" />
        <activity android:name=".DownloadRoom.IntoNewRoomActivity"></activity>
    </application>

</manifest>